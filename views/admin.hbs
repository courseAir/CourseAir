



            <div class=" container container-sm px-4">
                 <div class="row my-5">
                    <h3 class="fs-4 mb-3">Tracking Info</h3>
                    <div class="col col-sm ">
                    <div class="table-responsive">
                       <table class="table table-sm table-striped table-dark  table-hover text-center">
                            <thead>
                                <tr>
                                    
                                    <th scope="col">#</th>
                                    <th scope="col">First Name</th>
                                    <th scope="col">Last Name</th>
                                    <th scope="col">Course name</th>
                                    <th scope="col">Start date</th>
                                    <th scope="col">End date</th>
                                    <th scope="col">Duration left</th>
                                    <th scope="col"> Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each row1}}
                                  <tr>
                                    <th scope="row1">{{this.service_num}}</th>
                                    <td>{{this.first_name}}</td>
                                    <td>{{this.last_name}}</td>
                                    <td>{{this.course_name}}</td>
                                    <td>{{this.start_date}}</td>
                                    <td>{{this.end_date}}</td>
                                    <td>{{this.timeleft}}</td>
                                    <td class="text-end  ">
                                      
                                       <a href="/editInfo/{{this.service_num}}" type="button" class="btn btn-light "><i class="bi bi-pencil"></i>
                                          Edit</a>
                                      <a
                                        href="/{{this.service_num}}"
                                        type="button"
                                        class="btn btn-danger btn"
                                        ><i class="bi bi-person-x"></i>
                                        Delete</a>
                                      
                                    </td>
                            
                                  </tr>
                                {{/each}}
                            
                              </tbody>
                        </table>
                    </div>
                       
                    </div>
                </div>

            </div>
        