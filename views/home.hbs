


{{#if alert}}
  <div class="alert alert-success alert-dismissible fade show" role="alert">
     {{alert}}
    <button
      type="button"
      class="btn-close"
      data-bs-dismiss="alert"
      aria-label="Close"
    ></button>
  </div>
{{/if}}





            <div class="container-fluid">
                 <div class="row ">
                    <h3 class="fs-4 mb-3">Tracking Info</h3>
                    <div class="col">
                        <table class="table table-striped table-dark  table-hover table-sm">
                            <thead>
                                <tr>
                                    
                                    <th scope="col">#</th>
                                    <th scope="col">First Name</th>
                                    <th scope="col">Last Name</th>
                                    <th scope="col">Course name</th>
                                    <th scope="col">Start date</th>
                                    <th scope="col">End date</th>
                                    <th scope="col">Duration left</th>
                                    
                                </tr>
                            </thead>
                            <tbody>
                                {{#each row1}}
                                  <tr>
                                    <th scope="row1">{{this.service_num}}</th>
                                    <td>{{this.first_name}}</td>
                                    <td>{{this.last_name}}</td>
                                    <td>{{this.course_name}}</td>
                                    <td>{{this.start_date}}</td>
                                    <td>{{this.end_date}}</td>
                                    <td>{{this.timeleft}}</td>
                                                               
                                  </tr>
                                {{/each}}
                            
                              </tbody>
                        </table>
                    </div>
                </div>

            </div>
        